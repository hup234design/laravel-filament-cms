{
    "name": "hup234design/laravel-filament-cms",
    "type": "project",
    "description": "CMS plugin for Filament Admin.",
    "keywords": [
        "framework",
        "laravel",
        "filament",
        "cms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Walker",
            "email": "dave@hup234design.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "filament/filament": "^2.0",
        "filament/spatie-laravel-settings-plugin": "^2.0",
        "jeffgreco13/filament-breezy": "v1.4.10.1",
        "laravel/pint": "^1.2",
        "phpsa/filament-authentication": "^2.3",
        "plank/laravel-mediable": "^5.8",
        "ryangjchandler/filament-navigation": "^0.4.2",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/image": "^2.2",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "autoload": {
        "psr-4": {
            "Hup234design\\FilamentCms\\": "src/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hup234design\\FilamentCms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hup234design\\FilamentCms\\FilamentCmsServiceProvider"
            ],
            "aliases": {
                "FilamentCms": "Hup234design\\FilamentCms\\Facades\\FilamentCms"
            }
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
